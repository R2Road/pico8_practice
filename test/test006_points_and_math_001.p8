pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
px = 64
py = 64

function _update()

	cls()
	
	for py = 0, 127, 3 do
		for px = 0, 127, 3 do
			local dist=sqrt( px * px + py * py)
			z=cos( dist - time() ) * 6
			pset( px, py + z, 6)	
		end
	end
	
end
