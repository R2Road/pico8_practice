pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
r = 64
r_offset = 40

function _update()

	cls()
	
	for py = -r+r_offset, r-r_offset, 4 do
		for px = -r+r_offset, r-r_offset, 4 do
			local dist=sqrt( px * px + py * py)
			
			z_2=cos( dist/40 - time() ) * 6
			pset( r + px, r+ py + z_2, 6)
		end
	end
	
end
