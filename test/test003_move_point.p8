pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
px = 0
py = 0
speed = 6

function _draw()
	cls()
	
	pset( px, py, 6)	
end

function _update60()

	px += speed
	
	if px > 127 then
		px = 0
		py += speed
	end
	
	if py > 127 then
		py = 0
	end
	
end
