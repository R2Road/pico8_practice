pico-8 cartridge // http://www.pico-8.com
version 27
__lua__
r = 64
dist_range = 24
offset = r - dist_range

function _update()

	cls()
	
	for py = -r+offset, r-offset, 4 do
		for px = -r+offset, r-offset, 4 do
			local dist=sqrt( px * px + py * py)
			
			if dist > dist_range then
				goto continue
			end
			
			z_2=cos( dist/40 - time() ) * 6
			pset( r + px, r+ py + z_2, 6)
			
			::continue::
			
		end
	end
	
end
